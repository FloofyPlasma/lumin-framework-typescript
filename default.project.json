{
    "name": "roblox-ts-game",
    "emitLegacyScripts": false,
    "globIgnorePaths": [
        "**/package.json",
        "**/tsconfig.json"
    ],
    "tree": {
        "$className": "DataModel",
        "ServerStorage": {
            "TS": {
                "$path": "out/server"
            }
        },
        "ReplicatedStorage": {
            "rbxts_include": {
                "$path": "include",
                "node_modules": {
                    "$className": "Folder",
                    "@rbxts": {
                        "$path": "node_modules/@rbxts"
                    }
                }
            },
            "Shared": {
                "$className": "Folder",
                "TS": {
                    "$path": "out/shared"
                }
            },
            "Client": {
                "$className": "Folder",
                "TS": {
                    "$path": "out/client"
                }
            }
        }
    }
}
